<resources xmlns:android="http://schemas.android.com/tools">


    <style name="Theme.AppCompat.Light.NoActionBar.FullScreen" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>


    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="colorPrimary">@color/myPrimaryColor</item>
        <item name="colorPrimaryDark">@color/myPrimaryDarkColor</item>
        <item name="android:textColorPrimary">@color/myTextPrimaryColor</item>
        <item name="android:textColorSecondary">@color/Red</item>
        <item name="theme">@style/ThemeOverlay.AppCompat.Dark.ActionBar</item>
        <item name="colorAccent">@color/myPrimaryColor</item>
        <item name="android:editTextStyle">@style/EditTextStyle</item>
        <item name="android:textColor">@color/myTextPrimaryColor</item>

        <item name="android:actionBarDivider">@style/Widget.AppCompat.ActionBar</item>
        <item name="alertDialogTheme">@style/AppCompatAlertDialogStyle</item>

    </style>

    <style name="EditTextStyle" parent="Widget.AppCompat.EditText" />


    <style name="EditText">
        <item name="android:background">@drawable/edittext_bg</item>
        <item name="android:textColor">@color/Black</item>
        <item name="android:textSize">12sp</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="Button">
        <item name="android:background">@drawable/button_corner</item>
        <item name="android:textColor">@color/White</item>
        <item name="android:textSize">25sp</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="RadioButtonTeal" parent="Widget.AppCompat.CompoundButton.RadioButton">
        <item name="colorAccent">@color/actionbar_color</item>
        <item name="colorPrimary">@color/AliceBlue</item>
        <item name="android:textColor">@color/Black</item>

    </style>

    <style name="CheckButtonTeal" parent="Widget.AppCompat.CompoundButton.CheckBox">
        <item name="colorAccent">@color/actionbar_color</item>
        <item name="colorPrimary">@color/Red</item>
        <item name="android:textColor">@color/Black</item>

    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Widget.AppCompat.ActionBar." parent="Widget.AppCompat.ActionBar">
        <item name="actionBarDivider">@android:drawable/divider_horizontal_dark</item>
    </style>

    <style name="AppCompatAlertDialogStyle" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">#FFCC00</item>
        <item name="android:textColorPrimary">#000</item>
        <item name="android:background">#5fa3d0</item>
    </style>

    <style name="listSummery">
        <item name="android:layout_gravity">center</item>
        <item name="android:textColor">@color/ForestGreen</item>
        <item name="android:background">@drawable/border_radius_min</item>
        <!--<item name="android:padding">3dp</item>-->
        <item name="android:layout_margin">2dp</item>
    </style>


